{
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=8192"
  },
  "build": {
    "env": {
      "NEXT_LINT": "false"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "regions": ["iad1"],
  "crons": []
}